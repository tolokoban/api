    },
    "dependencies": {
        "@tolokoban/type-guards": "^0.6.0",
        "chalk": "^5.3.0",
        "json5": "^2.2.3",
        "readline-sync": "^1.4.10"
    },
    "devDependencies": {
        "@types/node": "^22.6.1",
        "@types/readline-sync": "^1.4.8",
        "fork-ts-checker-webpack-plugin": "^7.3.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.2",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-shebang-plugin": "^1.1.8"
