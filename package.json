    },
    "dependencies": {
        "chalk": "^5.2.0",
        "json5": "^2.2.1",
        "readline-sync": "^1.4.10"
    },
    "devDependencies": {
        "@types/node": "^18.11.15",
        "@types/readline-sync": "^1.4.4",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-shebang-plugin": "^1.1.8"
