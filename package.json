{
    "name": "@tolokoban/api",
    "version": "0.9.0",
    "description": "Quick Typescript code generator for simple and robust RPC applications based on a subset of REST protocol.",
    "private": false,
    "bin": {
        "api": "./dist/api.cjs"
    },
    "type": "module",
    "scripts": {
